@model EntityLayer.Concrete.Service
@{
    ViewData["Title"] = "AddService";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<h2>Yeni Servis ekleme</h2>
<br />
<form class="form-group" method="post">
    @Html.LabelFor(x => x.Title)
    @Html.TextBoxFor(x => x.Title, new { @class = "form-control" })
    <br />
   
    @Html.LabelFor(x => x.Content)
    @Html.TextAreaFor(x => x.Content, new { @class = "form-control" })
    <br />
    @Html.LabelFor(x => x.Service_image)
    @Html.TextBoxFor(x => x.Service_image, new { @class = "form-control" })
    <br />
    <button class="btn btn-info">Kaydet</button>

</form>
